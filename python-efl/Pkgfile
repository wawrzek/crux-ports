# Description: python binding for efl libraries (for DR19)
# URL: http://www.enlightenment.org/
# Maintainer: Wawrzek Niewodniczanski, main at wawrzek dot name
# Packager: Victor Martinez, pitillo at ono dot com
# Depends on: python, elementary

name=python-efl
version=1.18.0
release=1
source=(http://download.enlightenment.org/rel/bindings/python/$name-$version.tar.gz)

build()
{
    export DISABLE_CYTHON=1
    cd $name-$version
    python setup.py build
    python setup.py install --prefix=/usr --root=${PKG}
}
